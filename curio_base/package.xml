<?xml version="1.0"?>
<package format="2">
    <name>curio_base</name>
    <version>0.2.2</version>
    <description>Hardware controllers for Curio, a Swappy Rover</description>
    <maintainer email="rhys.mainwaring@me.com">Rhys Mainwaring</maintainer>
    <license>BSD-3-Clause</license>
    <url type="website">https://github.com/srmainwaring/curio</url>
    <url type="repository">https://github.com/srmainwaring/curio</url>
    <url type="bugtracker">https://github.com/srmainwaring/curio/issues</url>
    <author email="rhys.mainwaring@me.com">Rhys Mainwaring</author>

    <buildtool_depend>catkin</buildtool_depend>
    <buildtool_depend>message_generation</buildtool_depend>

    <depend>actionlib</depend>
    <depend>actionlib_msgs</depend>
    <depend>controller_manager</depend>
    <depend>curio_description</depend>
    <depend>curio_control</depend>
    <depend>curio_msgs</depend>
    <depend>geometry_msgs</depend>
    <depend>hardware_interface</depend>
    <depend>joint_state_publisher</depend>
    <depend>lx16a</depend>
    <depend>lx16a_msgs</depend>
    <depend>nav_msgs</depend>
    <!-- <depend>pybind11-dev</depend> -->
    <depend>python-serial</depend>
    <depend>robot_localization</depend>
    <depend>robot_state_publisher</depend>
    <depend>roscpp</depend>
    <depend>rospy</depend>
    <depend>serial</depend>
    <depend>sensor_msgs</depend>
    <depend>std_msgs</depend>
    <depend>tf</depend>

    <exec_depend>message_runtime</exec_depend>

</package>
